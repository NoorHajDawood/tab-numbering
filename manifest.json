{
  "manifest_version": 3,
  "name": "Tab Numbering",
  "description": "Displays the tab number (in superscript) for all open tabs.",
  "version": "0.2.0",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "tab-numbering.png"
  },
  "action": {
    "default_title": "Tab Numbering"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "@tab-numbering.NoorHajDawood",
      "strict_min_version": "109.0"
    }
  }
}
